#: backends/dbus/DBusHelperProxy.cpp:74
#, qt-format
msgctxt "KAuth::DBusHelperProxy|"
msgid "DBus Backend error: service start %1 failed: %2"
msgstr "DBus-feil: klarte ikkje starta tenesta %1: %2"

#: backends/dbus/DBusHelperProxy.cpp:89
#, qt-format
msgctxt "KAuth::DBusHelperProxy|"
msgid ""
"DBus Backend error: connection to helper failed. %1\n"
"(application: %2 helper: %3)"
msgstr ""
"D-Bus-feil: Klarte ikkje kopla til hjelparen. %1\n"
"(program: %2; hjelpar: %3)"

#: backends/dbus/DBusHelperProxy.cpp:124
#, qt-format
msgctxt "KAuth::DBusHelperProxy|"
msgid ""
"DBus Backend error: could not contact the helper. Connection error: %1. "
"Message error: %2"
msgstr ""
"D-Bus-feil: Klarte ikkje kontakta hjelparen. Sambandsfeil: %1. Meldingsfeil: "
"%2"

#: executejob.cpp:86
msgctxt "KAuth::ExecuteJob|"
msgid "Tried to start an invalid action"
msgstr "Prøvde å starta ei ugyldig handling"

#: executejob.cpp:104
msgctxt "KAuth::ExecuteJob|"
msgid "Unknown execution mode chosen"
msgstr "Ukjend køyremodus vald"

#: executejob.cpp:149
msgctxt "KAuth::ExecuteJob|"
msgid "Unknown status for the authentication procedure"
msgstr "Ukjend status for autentiseringsprosedyren"

#: executejob.cpp:161
msgctxt "KAuth::ExecuteJob|"
msgid ""
"The current backend only allows helper authorization, but this action does "
"not have a helper."
msgstr ""
"Den gjeldande motoren tillèt berre hjelparautorisering, men denne handlinga "
"har ikkje nokon hjelpar."

#: executejob.cpp:169 executejob.cpp:193
msgctxt "KAuth::ExecuteJob|"
